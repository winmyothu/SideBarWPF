<UserControl x:Class="MMDictionary.View.About"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MMDictionary.View"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="622">
    <Grid>
        <Border CornerRadius="5 0 0 0"
                Background="#ffffff"></Border>
        <StackPanel>
            <TextBlock Text="Welcome to About Page" Foreground="LightPink" Margin=" 30"
                                       Style="{StaticResource Text_Style}" />

        </StackPanel>
    </Grid>
</UserControl>
